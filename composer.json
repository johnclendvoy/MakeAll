{
    "name": "johnclendvoy/makeall",
    "description": "A Laravel command to easily create all of the needed files for a new model.",
    "license": "MIT",
    "authors": [
        {
            "name": "John C. Lendvoy",
            "email": "john.c.lendvoy@gmail.com",
            "homepage": "https://johnclendvoy.ca"
        }
    ],
    "homepage": "https://github.com/johnclendvoy/makeall",
    "keywords": ["Laravel", "MakeAll"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "johnclendvoy\\MakeAll\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "johnclendvoy\\MakeAll\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "johnclendvoy\\MakeAll\\MakeAllServiceProvider"
            ],
            "aliases": {
                "MakeAll": "johnclendvoy\\MakeAll\\Facades\\MakeAll"
            }
        }
    }
}